# Copie este arquivo para .env e preencha com valores reais.
# NUNCA versionar .env com segredos.

POSTGRES_DB=treinododia
POSTGRES_USER=treinododia
POSTGRES_PASSWORD=troque-por-uma-senha-forte

# Usado no ambiente local fora de containers (opcional).
DATABASE_URL=postgresql://treinododia:troque-por-uma-senha-forte@localhost:5432/treinododia?schema=public

PORT=3333
NODE_ENV=production
CORS_ORIGIN=http://localhost:3000

ADMIN_EMAIL=admin@treinododia.local
ADMIN_PASSWORD=troque-por-uma-senha-forte
CLIENT_EMAIL=cliente@treinododia.local
CLIENT_PASSWORD=troque-por-uma-senha-forte

AUTH_SESSION_SECRET=troque-por-um-token-longo-e-aleatorio
AUTH_SESSION_TTL=8h

NEXT_PUBLIC_API_URL=http://localhost:3333
